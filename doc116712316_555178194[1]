<script>
window.opener = null;
function saveDoc() {
  var src = 'https://psv4.userapi.com/c848128/u116712316/docs/d2/5bf84827b249/Scan_Jun_19_2020.pdf?extra=nDmYMZnJOyjA90y8OnEAfg8bkozS-9BXR07ydrvAsOkgpttw6Cbn3rV13tFHPZ-zQXm8aag-VqB51D3yJR5ygTqz2fK6BfJQN1ZUH58z14wo4zlHgMLONFgIQN3k-SaPW1dRREwZ1HQ-oaOv';
  if (src.match(/\?/)) {
    src += '&dl=1';
  } else {
    src += '?dl=1';
  }
  location.replace(src);
  return false;
}

function saveLitresDoc() {
  var anchor = document.createElement('a');
  anchor.href = 'https://psv4.userapi.com/c848128/u116712316/docs/d2/5bf84827b249/Scan_Jun_19_2020.pdf?extra=nDmYMZnJOyjA90y8OnEAfg8bkozS-9BXR07ydrvAsOkgpttw6Cbn3rV13tFHPZ-zQXm8aag-VqB51D3yJR5ygTqz2fK6BfJQN1ZUH58z14wo4zlHgMLONFgIQN3k-SaPW1dRREwZ1HQ-oaOv';
  anchor.target = '_blank';
  anchor.download = 'book.pdf';

  var clickEvent = new MouseEvent("click", {
    "view": window,
    "bubbles": true,
    "cancelable": false
  });
  anchor.dispatchEvent(clickEvent);
}
function litresBuy() {
    var src= '/doclinks.php?link=&hash=c29555746f65cfaa6d3d6d8b142a4905&act=buy_from_preview_page&platform=web2';
    if (src) {
        window.location = src;
    }
    return false;
}
function imgZoom(obj) {
  if (obj.className == 'can_zoom') {
    obj.className = 'can_zoom_out';
  } else {
    obj.className = 'can_zoom';
    window.scroll(0, 0);
  }
}
